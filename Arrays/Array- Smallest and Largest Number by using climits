//Arrays Smallest and Largest Number
#include<iostream>
#include<climits>
using namespace std;
int main()
{
	int n,key;
	int a[1000];
	cin>>n;
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}
	int smallest = INT_MAX;
	int largest = INT_MIN;
	for(int i=0;i<n;i++)
	{
		if(a[i]<smallest)
		{
			smallest = a[i];
		}
		
		else if(a[i]>largest)
		{
			largest = a[i];
		}
		
	}	
	
	cout<<"LARGEST : "<<largest<<" "<<"SMALLEST : "<<smallest;
	return 0;
	
}
